{
  "regions": [
    "syd1"
  ],
  "functions": {
    "api/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "ENABLE_FILE_SYSTEM_API": "1"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}